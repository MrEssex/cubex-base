{
  "name": "mr-essex/cubex-base",
  "description": "Base for Cubex Projects",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Kyle Essex",
      "email": "kylegavinessex@hotmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "auto"
  },
  "autoload": {
    "psr-4": {
      "CubexBase\\Application\\": "src"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-apcu": "*",
    "ext-json": "*",
    "cubex/framework": "~4.0",
    "filp/whoops": "^2.3",
    "mressex/file-cache": "^0.1.0",
    "packaged-ui/bem-component": "^1.1",
    "packaged/dal": "^1.9",
    "packaged/dispatch": "^2.11.0",
    "packaged/glimpse": "^2.7",
    "packaged/http": "^1.16"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
