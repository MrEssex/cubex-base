{
  "name": "mr-essex/cubex-base",
  "description": "Base for Cubex Projects",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "alpha",
  "authors": [
    {
      "name": "Kyle Essex",
      "email": "kylegavinessex@hotmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": "auto"
  },
  "autoload": {
    "psr-4": {
      "CubexBase\\Application\\": "src"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-apcu": "*",
    "ext-json": "*",
    "cubex/framework": "~4.0",
    "statickidz/php-google-translate-free": "^1.1.1",
    "filp/whoops": "^2.3",
    "maxmind-db/reader": "^v1.9.0",
    "mressex/file-cache": "^0.1.0",
    "packaged-ui/bem-component": "^1.1",
    "packaged-ui/fontawesome": "^3.5.0",
    "packaged-ui/pagelets": "dev-master",
    "packaged/dal": "^1.9",
    "packaged/dispatch": "^2.11.0",
    "packaged/glimpse": "^2.7",
    "packaged/http": "^1.16",
    "packaged/rwd": "^1.4.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
